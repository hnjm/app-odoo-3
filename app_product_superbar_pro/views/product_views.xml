<odoo>
  <data>
      <!-- Product -->
    <record id="app_product_template_search_view_pro" model="ir.ui.view">
        <field name="name">app.product.template.search</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="app_product_superbar.app_product_template_search_view"/>
        <field name="arch" type="xml">
            <xpath expr="//searchpanel//field[@name='type']" position="after">
                <field name="sale_ok"/>
                <field name="purchase_ok"/>
            </xpath>
        </field>
    </record>
  </data>
</odoo>